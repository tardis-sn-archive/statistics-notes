Cell["Return first element >= key", "Section"],

Cell[BoxData[
 RowBox[{
  RowBox[{"binSearchMin", "[", 
   RowBox[{"list_List", ",", " ", "key_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"low", "=", "1"}], ",", 
      RowBox[{"high", "=", 
       RowBox[{"Length", "[", "list", "]"}]}], ",", "mid", ",", "midVal"}], 
     "}"}], ",", "\n", 
    RowBox[{"(*", " ", 
     RowBox[{
     "first", " ", "element", " ", "is", " ", "the", " ", "right", " ", 
      "one"}], " ", "*)"}], "\n", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"First", "[", "list", "]"}], ">=", "key"}], ",", 
       RowBox[{"Return", "[", "1", "]"}]}], "]"}], ";", "\n", 
     RowBox[{"(*", " ", 
      RowBox[{
      "key", " ", "larger", " ", "than", " ", "everything", " ", "in", " ", 
       "list"}], " ", "*)"}], "\n", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Last", "[", "list", "]"}], "<", "key"}], ",", 
       RowBox[{"Return", "[", "0", "]"}]}], "]"}], ";", "\n", "\n", 
     RowBox[{"(*", " ", 
      RowBox[{
      "else", " ", "it", " ", "is", " ", "inside", " ", "the", " ", "list"}], 
      " ", "*)"}], "\n", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"low", "<", " ", 
        RowBox[{"high", "-", "1"}]}], ",", "\n", 
       RowBox[{
        RowBox[{"mid", "=", 
         RowBox[{"Quotient", "[", 
          RowBox[{
           RowBox[{"low", "+", "high"}], ",", "2"}], "]"}]}], ";", "\n", 
        RowBox[{"midVal", " ", "=", " ", 
         RowBox[{"list", "[", 
          RowBox[{"[", "mid", "]"}], "]"}]}], ";", "\n", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"Print", "[", 
           RowBox[{"{", 
            RowBox[{"mid", ",", "midVal"}], "}"}], "]"}], ";"}], "*)"}], "\n", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"midVal", ">=", " ", "key"}], ",", 
          RowBox[{"high", "=", "mid"}], ",", "\n", 
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"midVal", "<", "key"}], ",", 
             RowBox[{"low", "=", "mid"}]}], "]"}], ";"}]}], "\n", "]"}], 
        ";"}]}], "\n", "]"}], ";", "\n", "high"}]}], "\n", "]"}]}]], "Input"]
}, Open  ]],

